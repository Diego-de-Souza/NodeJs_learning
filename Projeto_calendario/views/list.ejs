<h3>Consultas: </h3>
<hr>
<form action="" method="get">
    <input type="search" name="" id="" placeholder="Digite o email ou cpf do cliente">
    <button>Pesquisar</button>
</form>
<% appos.forEach(appo =>{ %>
    <h1>Consulta: <%= appo.description %></h1>
    <h4>Paciente: <%= appo.name %></h4>
    <h4>E-mail: <%= appo.email %></h4>
    <h4>CPF: <%= appo.cpf %> </h4>
    <hr>
<% }) %>